{
  "name": "bunny-skia",
  "version": "0.0.10",
  "private": false,
  "type": "module",
  "description": "canvaskit-wasm for Edge Scripting for bunny.net ðŸ°",
  "keywords": [
    "BunnyCDN",
    "bunny.net",
    "bunny",
    "cdn",
    "bunny-launcher",
    "typescript",
    "edge-scripting",
    "edge",
    "scripting",
    "canvaskit",
    "skia",
    "wasm"
  ],
  "homepage": "https://bunny-launcher.net/imaging/skia/",
  "bugs": "https://github.com/jlarmstrongiv/bunny-skia/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlarmstrongiv/bunny-skia.git"
  },
  "license": "AGPL-3.0-only",
  "author": "John L. Armstrong IV",
  "sideEffects": false,
  "scripts": {
    "build:dev": "NODE_ENV=development npx tsx src/bundler",
    "build": "npx tsx src/bundler",
    "patch:canvaskit": "npx patch-package canvaskit-wasm --exclude='nothing'",
    "patch:webgpu": "npx patch-package @webgpu/types --exclude='nothing'",
    "postinstall:local": "npx patch-package",
    "prepublishOnly": "npm run build",
    "pretest:deno": "npm run build:dev",
    "pretest:node": "npm run build:dev",
    "test:deno": "deno run --allow-all src/test/index.test.tsx",
    "test:node": "npx tsx src/test/index.test.tsx"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@webgpu/types": "0.1.21"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@types/fs-extra": "11.0.4",
    "@types/node": "22.5.4",
    "canvaskit-wasm": "0.39.1",
    "esbuild": "0.24.0",
    "fs-extra": "11.2.0",
    "patch-package": "8.0.0",
    "tsx": "4.19.1",
    "typescript": "5.5.4"
  },
  "packageManager": "npm@10.9.0",
  "engines": {
    "node": ">=20"
  }
}
